{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "lib": ["ES5", "ES6", "ESNext", "DOM"],
    "isolatedModules": true,
    "jsx": "react-jsx",
    "noEmit": true
  }
}
